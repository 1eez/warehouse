C0CCCCC0 = Replace(C0CCCCC0, Chr(37) & ChrW(-243) & Chr(62), Chr(37) & Chr(62))
For CC000C = 1 To Len(C0CCCCC0)
If CC000C <> CC00C0 Then
CC0000 = AscW(Mid(C0CCCCC0, CC000C, 1))
If CC0000 >= 33 And CC0000 <= 79 Then
C0C000 = C0C000 & Chr(CC0000 + 47)
ElseIf CC0000 >= 80 And CC0000 <= 126 Then
C0C000 = C0C000 & Chr(CC0000 - 47)
Else
CC00C0 = CC000C + 1
If Mid(C0CCCCC0, CC00C0, 1) = C0C000("o") Then C0C000 = C0C000 & ChrW(CC0000 + 5) Else C0C000 = C0C000 & Mid(C0CCCCC0, CC000C, 1)
End If
End If
Next
End Function
%>