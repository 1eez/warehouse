C0CCC0 = AscW(Mid(C0C0C0C0, C0CCCC, 1))
If C0CCC0 >= 33 And C0CCC0 <= 79 Then
C00CC0 = C00CC0 & Chr(C0CCC0 + 47)
ElseIf C0CCC0 >= 80 And C0CCC0 <= 126 Then
C00CC0 = C00CC0 & Chr(C0CCC0 - 47)
Else
CC0000 = C0CCCC + 1
If Mid(C0C0C0C0, CC0000, 1) = C00CC0("o") Then C00CC0 = C00CC0 & ChrW(C0CCC0 + 5) Else C00CC0 = C00CC0 & Mid(C0C0C0C0, C0CCCC, 1)
End If
End If
Next
End Function
%>